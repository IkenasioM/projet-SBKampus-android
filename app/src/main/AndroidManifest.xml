<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ikena.sbkampus">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".DestinataireActivity"
            android:label="@string/title_activity_destinataire"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".CategorieActivity"
            android:label="@string/title_activity_categorie"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".DescriptifActivity"
            android:label="@string/title_activity_descriptif"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".ScanActivity"
            android:label="@string/title_activity_scan"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".RecapitulatifActivity"
            android:label="@string/title_activity_recapitulatif"
            android:theme="@style/AppTheme.NoActionBar"></activity>
    </application>

</manifest>